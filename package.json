{
  "name": "startreact",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "uikit": "^3.0.0-beta.42"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/tema/ -o src/tema/",
    "watch-css": "yarn build-css && node-sass-chokidar src/tema/ -o src/tema/ --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.3.0"
  }
}
